source 'http://rubygems.org'

require 'json'
require 'open-uri'
versions = JSON.parse(open('https://pages.github.com/versions.json').read)

gem 'github-pages', versions['github-pages']

gem "jekyll"
gem "jekyll-paginate"
gem 'jekyll-paginate-category'
gem "susy"
gem "normalize-scss"
gem "compass"
gem "sass"
gem "support-for"
